                <processor type="ActiveCommerce.OrderProcessing.CheckPaymentStatus, ActiveCommerce.Kernel">
                    <patch:attribute name="type">ActiveCommerce.Training.FailedPaymentMessage.CheckPaymentStatus, ActiveCommerce.Training.FailedPaymentMessage</patch:attribute>
                </processor>
